{
  "version": 2,
  "builds": [
    {
      "src": "studyBud/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "srcDir": ".",
        "pythonVersion": "3.9"
      }
    },
    {
      "src": "Frontend/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "studyBud/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "Frontend/static/$1"
    }
  ],
  "env": {
    "POSTGRES_DB": "studyBud",
    "POSTGRES_USER": "yike",
    "POSTGRES_PASSWORD": "yikeber50",
    "POSTGRES_HOST": "localhost",
    "VIRTUAL_ENV": "/venv"
  }
}
